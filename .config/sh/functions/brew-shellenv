#!/bin/sh

if test -z "${HOMEBREW_PREFIX+_}"; then
  case "$(uname)" in
    Darwin)
      eval "$(PATH=/opt/homebrew/bin:/usr/local/bin SHELL=/bin/sh brew shellenv)"
      ;;
    Linux)
      eval "$(PATH="$HOME/.linuxbrew/bin:/home/linuxbrew/.linuxbrew/bin" SHELL=/bin/sh brew shellenv)"
      ;;
  esac
fi
